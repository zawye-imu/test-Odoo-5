<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record id="view_res_users_role_form_http_auth" model="ir.ui.view">
    <field name="name">view.res.users.role.form.http.auth</field>
    <field name="model">res.users.role</field>
    <field name="inherit_id" ref="base_user_role.view_res_users_role_form"/>
    <field name="arch" type="xml">
      <field name="group_id" position="after">
        <field name="user_role_code"/>
      </field>
    </field>
  </record>

  <record id="view_res_users_role_tree_http_auth" model="ir.ui.view">
    <field name="name">view.res.users.role.tree.http.auth</field>
    <field name="model">res.users.role</field>
    <field name="inherit_id" ref="base_user_role.view_res_users_role_tree"/>
    <field name="arch" type="xml">
      <field name="user_ids" position="after">
        <field name="user_role_code"/>
      </field>
    </field>
  </record>

</odoo>
